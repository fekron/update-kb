subject_domain_of_asian_cuisine
    =>nrel_main_idtf:[Предметная область азиатской кухни] (* <-lang_ru;;*);
    =>nrel_main_idtf:[Subject domain of asian cuisine] (* <-lang_en;;*);
    <-subject_domain; 
	<-subject_domain_of_cuisine; 
    ->rrel_maximum_studied_object_class: concept_asian_cuisine;

    ->rrel_not_maximum_studied_object_class: 
        concept_dango;
        concept_misoshiru; 
        concept_omurice; 
        concept_oyakodon; 
        concept_ramen; 
        concept_sashimi;

    =>nrel_section_decomposition:{
        section_subject_domain_of_japanese_cuisine;
        section_subject_domain_of_chinese_cuisine;
        section_subject_domain_of_korean_cuisine;
        section_subject_domain_of_indian_cuisine; 
        section_subject_domain_of_thai_cuisine;
        section_subject_domain_of_vietnam_cuisine;
        section_subject_domain_of_malaysian_cuisine;
        section_subject_domain_of_filipino_cuisine
    };;